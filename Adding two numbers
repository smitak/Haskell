module Main where

doublefactorial 0 = 1;
doublefactorial 1 = 1;
doublefactorial n = n * doublefactorial(n-1);

add x = x + x;

main = do
       putStrLn "Hello World";
       let fact  = doublefactorial 4;
       print fact;
       let sum = add 4;
       print sum;
       putStrLn "Ths is working";
